<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!-- 2.5版本之后 uri="http://java.sun.com/jsp/jstl/core"  且只需引入 jstl-1.2.jar 到 lib中, 不再需要standard  -->
<%@ taglib uri="http://java.sun.com/jsp/jstl/core"  prefix="c"%>
<!DOCTYPE HTML>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Add Product Form 3</title>
</head>
<body>
	<div>
		<h4>app3</h4>
		<form action="product_save.action" method="post">
			<fieldset>
				<legend>Add a product</legend>
				<p>
					<label for="name">Product Name:</label>
					<input type="text" id="name" name="name" tabindex="1" value="">
				</p>
				<p>
					<label for="description">Description:</label>
					<input type="text" id="description" name="description" tabindex="2" value="">
				</p>
				<p>
					<label for="price">Price:</label>
					<input type="text" id="price" name="price" tabindex="3" value="">
				</p>
				<p>
					<input id="reset" type="reset" tabindex="4">
					<input id="submit" type="submit" tabindex="5" value="Add Product">
				</p>
			</fieldset>
		</form>
	</div>
</body>
</html>